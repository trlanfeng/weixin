.water-jar .water-filling {
    position: absolute;
    left: 0;
    top: 0;
    z-index: 3;
    width: 100%;
    height: 100%;
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    display: -webkit-flex;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    justify-content: center;
    color: #fff;
    font-size: 64px;
}

.water-jar .water {
    position: absolute;
    left: 0;
    top: 0;
    z-index: 2;
    width: 100%;
    height: 100%;
    transform: translate(0,100%);
    background: #E8EEF9;
}

.water-jar .water_wave {
    width: 200%;
    position: absolute;
    bottom: 100%;
}

.water-jar .water_wave_back {
    height: 19px;
    width: 560px;
    right: 0;
    fill: #C7EEFF;
    animation: wave-back 1.4s infinite linear;
}

.water-jar .water_wave_front {
    height: 20px;
    width: 560px;
    left: 0;
    fill: #4D6DE3;
    margin-bottom: -1px;
    animation: wave-front .7s infinite linear;
}

canvas.water-jar.drawingCanvas {
    border: 1px solid #D8D8D8;
    background-color: white;
    border-radius: 24px 24px 0px 0px;
    box-shadow: 0 -1px 12px 0 rgba(0,0,0,0.24);
}

@-webkit-keyframes wave-front {
    100% {
        transform: translate(-50%,0);
    }
}

@keyframes wave-front {
    100% {
        transform: translate(-50%,0);
    }
}

@-webkit-keyframes wave-back {
    100% {
        transform: translate(50%,0);
    }
}

@keyframes wave-back {
    100% {
        transform: translate(50%,0);
    }
}

@-webkit-keyframes spaceboots {
    0% {
        transform: translate(2px,1px) rotate(0deg);
    }

    10% {
        transform: translate(-1px,-2px) rotate(-1deg);
    }

    20% {
        transform: translate(-3px,0px) rotate(1deg);
    }

    30% {
        transform: translate(0px,2px) rotate(0deg);
    }

    40% {
        transform: translate(1px,-1px) rotate(1deg);
    }

    50% {
        transform: translate(-1px,2px) rotate(-1deg);
    }

    60% {
        transform: translate(-3px,1px) rotate(0deg);
    }

    70% {
        transform: translate(2px,1px) rotate(-1deg);
    }

    80% {
        transform: translate(-1px,-1px) rotate(1deg);
    }

    90% {
        transform: translate(2px,2px) rotate(0deg);
    }

    100% {
        transform: translate(1px,-2px) rotate(-1deg);
    }
}

@keyframes spaceboots {
    0% {
        transform: translate(2px,1px) rotate(0deg);
    }

    10% {
        transform: translate(-1px,-2px) rotate(-1deg);
    }

    20% {
        transform: translate(-3px,0px) rotate(1deg);
    }

    30% {
        transform: translate(0px,2px) rotate(0deg);
    }

    40% {
        transform: translate(1px,-1px) rotate(1deg);
    }

    50% {
        transform: translate(-1px,2px) rotate(-1deg);
    }

    60% {
        transform: translate(-3px,1px) rotate(0deg);
    }

    70% {
        transform: translate(2px,1px) rotate(-1deg);
    }

    80% {
        transform: translate(-1px,-1px) rotate(1deg);
    }

    90% {
        transform: translate(2px,2px) rotate(0deg);
    }

    100% {
        transform: translate(1px,-2px) rotate(-1deg);
    }
}

.shake {
    animation-name: spaceboots;
    animation-duration: 0.8s;
    transform-origin: 50% 50%;
    animation-iteration-count: infinite;
    animation-timing-function: linear;
}

div.callout {
    height: 15px;
    width: 80%;
    font-size: 12px;
    position: relative;
    padding: 8px;
    border-radius: 3px;
    border: 2px dotted #174EA6;
    margin-top: 10px;
}

.callout::before {
    content: "";
    width: 0px;
    height: 0px;
    border: 0.8em dotted transparent;
    position: absolute;
}

.callout.bottom-left::before {
    position: absolute;
    left: 80px;
    bottom: -8px;
    display: inline-block;
    border-top: 2px dotted #174EA6;
    border-right: 2px dotted #174EA6;
    width: 5px;
    height: 5px;
    transform: rotate(135deg);
